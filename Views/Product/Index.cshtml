        <div class="agile_inner_breadcrumb">
            <div class="container">
                <ul class="w3_short">
                    <li>
                        <i class="fa-solid fa-house"></i>
                        <a href="/Home">Trang chủ</a>
                        <i class="fa-solid fa-angle-right"></i>
                    </li>
                    <li>Sản phẩm</li>
                </ul>
            </div>
        </div>
    </div>
    <!-- //page -->
    <!-- top Products -->
    <div class="ads-grid py-sm-0 py-4">
        <div class="container py-xl-4 py-lg-2">
            @{
                var randomCategories = ((IEnumerable<ThuongMaiDienTu.Models.tb_ProductCategory>)ViewBag.Categories).OrderBy(c => Guid.NewGuid()).Take(12);
            }
            <div class="row py-4">
                @foreach (var category in randomCategories)
                {
                    <div class="col-sm-4 col-md-3 col-lg-2 mb-4">
                        <div class="product-category d-flex flex-column align-items-center">
                            <img src="/images/@Url.Content(category.Image)" alt="@category.Name" class="img-fluid mb-2" style="max-width: 80px; max-height: 80px;" />
                            <h4 class="text-center">@category.Name</h4>
                        </div>
                    </div>
                }
            </div>

            @{
                var categories = (IEnumerable<ThuongMaiDienTu.Models.tb_ProductCategory>)ViewBag.Categories;
                var mainCategories = categories.Where(c => c.ParentID == null);
            }

            <div class="row">
                <div class="col-lg-3 mt-lg-0 mt-4 p-lg-0">
                    <div class="side-bar p-sm-4 p-3">
                        <!-- Search -->
                        <div class="search-hotel border-bottom py-2">
                            <h3 class="agileits-sear-head mb-3">Search</h3>
                            <form action="#" method="post">
                                <input type="search" placeholder="Search..." name="search" required="">
                                <input type="submit" value=" ">
                            </form>
                        </div>
                        <!-- //Search -->
                        <!-- Category -->
                        <div class="search-hotel border-bottom py-2">
                            <h3 class="agileits-sear-head mb-3">Category</h3>
                            <div class="left-side py-2">
                                <ul class="main-categories">
                                    @foreach (var category in mainCategories)
                                    {
                                        <li class="@(categories.Any(c => c.ParentID == category.CateID) ? "has-children" : "")">
                                            <label for="@("parent_" + category.CateID)" class="toggle-arrow">
                                                <span class="span">@category.Name</span>
                                            </label>
                                            @if (categories.Any(c => c.ParentID == category.CateID))
                                            {
                                                var subCategories = categories.Where(c => c.ParentID == category.CateID);
                                                <ul class="sub-categories">
                                                    @foreach (var subcategory in subCategories)
                                                    {
                                                        <li>
                                                            <input type="checkbox" class="checked" id="@("child_" + subcategory.CateID)">
                                                            <label for="@("child_" + subcategory.CateID)">
                                                                <span class="span">@subcategory.Name</span>
                                                            </label>
                                                        </li>
                                                    }
                                                </ul>
                                            }
                                        </li>
                                    }
                                </ul>
                            </div>
                        </div>

                        <!-- //Category -->
